{
    "name": "konekt/eloquent-value-object",
    "description": "Value object casting for Eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": ["value-object", "konekt", "artkonekt", "laravel", "eloquent"],
    "support": {
        "source": "https://github.com/artkonekt/eloquent-value-object"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3",
        "illuminate/events": "5.*"
    },
    "autoload": {
        "psr-4": { "Konekt\\EloquentValueObject\\": "src/" },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\EloquentValueObject\\Tests\\": "tests/"
        }
    }
}
